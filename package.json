{
  "name": "react-blockly",
  "private": true,
  "description": "A React Blockly visual programming editor.",
  "author": "SerSerch <serserchit@gmail.com> (https://github.com/SerSerch)",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "packageManager": "yarn@4.2.2",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint \"./packages/**/src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit --composite false",
    "test": "jest",
    "clean": "lerna run clean --no-private",
    "build": "lerna run build --no-private",
    "release": "lerna publish --no-private",
    "web": "yarn workspace @react-blockly/web-example",
    "native": "yarn workspace @react-blockly/native-example"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@blockly/eslint-config": "^4.0.0",
    "@evilmartians/lefthook": "^1.6.12",
    "@react-native/eslint-config": "^0.74.83",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.72.8",
    "@types/react-native-get-random-values": "^1",
    "@types/uuid": "^9",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "blockly": "^11.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "lerna": "^8.1.3",
    "prettier": "^3.2.5",
    "react": "18.3.1",
    "react-native": "0.74.1",
    "react-native-builder-bob": "^0.23.2",
    "react-native-webview": "^13.10.2",
    "react-scripts": "^5.0.1",
    "sharp-cli": "^4.2.0",
    "typescript": "5.4.5"
  },
  "resolutions": {
    "@types/react": "^18.3.3"
  }
}
